{
  "manifest_version": 3,
  "name": "Autodesk Authorization Header Extractor",
  "description": "Extracts Autodesk authorization header from requests to Autodesk websites",
  "version": "1.4",
  "permissions": [
    "tabs", "clipboardWrite", "storage" ,"webRequest"
  ],
  "host_permissions": [
    "https://*.autodesk.com/*"
  ],
  "action": {
      "default_title": "Show header",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
